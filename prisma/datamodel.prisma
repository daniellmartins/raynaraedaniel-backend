type User {
  id: ID! @unique
  code: Int! @unique
  name: String!
  role: Role!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type Product {
  id: ID! @unique
  name: String!
  description: String
  price: Float!
  quantity: Int! @default(value: "1")
  photoUrl: String
  active: Boolean! @default(value: "true")
  createdAt: DateTime!
  updatedAt: DateTime!
}

enum Role {
  USER
  ADMIN
}
